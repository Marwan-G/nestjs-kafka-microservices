{
	"name": "@nestjs-kafka-microservice/notification-microservice-e2e",
	"version": "0.0.1",
	"private": true,
	"nx": {
		"implicitDependencies": [
			"@nestjs-kafka-microservice/notification-microservice"
		],
		"targets": {
			"e2e": {
				"executor": "@nx/jest:jest",
				"outputs": [
					"{projectRoot}/test-output/jest/coverage"
				],
				"options": {
					"jestConfig": "apps/notification-microservice-e2e/jest.config.ts",
					"passWithNoTests": true
				},
				"dependsOn": [
					"@nestjs-kafka-microservice/notification-microservice:build",
					"@nestjs-kafka-microservice/notification-microservice:serve"
				]
			}
		}
	}
}
